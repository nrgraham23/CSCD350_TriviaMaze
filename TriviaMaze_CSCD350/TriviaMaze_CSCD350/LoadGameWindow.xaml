<Window x:Class="TriviaMaze_CSCD350.LoadGameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LoadGameWindow" Height="209" Width="339" Topmost="True">
    <Grid>
        <Button x:Name="loadGameOKButton" Content="_OK" Margin="128,135,128,0" VerticalAlignment="Top" Click="loadGameOKButton_Click"/>
        <Label Content="Enter the name of a saved game file in the box below &#xA;and then click ok." HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="48" Width="311"/>
        <TextBox x:Name="fileNameTextBox" HorizontalAlignment="Left" Height="23" Margin="10,83,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="311"/>
        
    </Grid>
</Window>
